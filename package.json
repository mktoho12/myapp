{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec babel-node src",
    "test": "nyc mocha --require babel-core/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "sync": "babel-node ./src/models/sync.js",
    "compile": "babel src -d dist",
    "lint": "eslint src/** test/** frontend/js/**",
    "webpack": "webpack --config webpack.config.js",
    "css": "npm-run-all css-compile css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 frontend/scss/custom.scss public/stylesheets/custom.css",
    "css-prefix": "postcss --config node_modules/bootstrap/build/postcss.config.js --replace public/stylesheets/custom.css",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output public/stylesheets/custom.min.css public/stylesheets/custom.css",
    "build": "npm-run-all compile css webpack",
    "serve": "node dist",
    "heroku-postbuild": "yarn build"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "helmet": "^3.9.0",
    "morgan": "~1.9.0",
    "pg": "^7.4.1",
    "pug": "2.0.0-beta11",
    "sequelize": "^4.31.1",
    "serve-favicon": "~2.4.5",
    "validator": "^9.2.0",
    "zxcvbn": "^4.4.2"
  },
  "description": "えむけーのアプリ！",
  "main": "lib/app.js",
  "repository": "git@github.com:mktoho12/myapp.git",
  "author": "えむけー <makoto_16n1200102@nnn.ed.jp>",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.1.0",
    "mocha-junit-reporter": "^1.15.0",
    "nodemon": "^1.14.11",
    "nyc": "^11.4.1",
    "popper.js": "^1.12.9",
    "vue": "^2.5.13",
    "animejs": "^2.2.0",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "https://github.com/twbs/bootstrap",
    "cheerio": "^1.0.0-rc.2",
    "clean-css-cli": "^4.1.10",
    "lodash": "^4.17.4",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "open-iconic": "^1.1.1",
    "postcss": "^6.0.16",
    "postcss-cli": "^4.1.1",
    "supertest": "^3.0.0",
    "webpack": "^3.10.0"
  },
  "engines": {
    "node": "9.4.0",
    "yarn": "1.3.2"
  },
  "nodemonConfig": {
    "watch": "src"
  }
}
